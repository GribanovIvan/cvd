	   if (( $debug > 1 )); then echo fiecho target 4 done. code name: torrent;fi;fi
	   all=$(echo $resp | sed s/.*\"torrents\"\:\ \{//g | grep -o /upload/torrents | wc -l)
	   echo $resp > 123
	   if [ $mpv = 0 ] && [ $yad = 0 ]; then echo Найдено несколько торрентов.. Выберите один
	   temp=0
        until [ $temp = $all ];do
        let ' temp = temp + 1 '
        echo $temp\) Серии $(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*series//g | cut -d\} -f1 | rev | cut -d\" -f2 | rev), $(echo $resp sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*quality//g | cut -d\} -f1 | sed s/.*string//g | cut -d\" -f3), $(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*seeders//g | cut -c3- | cut -d\, -f1) сидов, $(bc<<<"scale=2;$(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*total_size\"://g | cut -d, -f1)/1073741824")GiB, $(date --date @$(echo $(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*uploaded_timestamp\"://g | cut -d, -f1)))
        done
	   read temp
	   $(grep application/x-bittorrent /usr/share/applications/mimeinfo.cache | cut -d\= -f2 | cut -d. -f2) http://cvd.anilib.top$(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*\"url\"\:\"//g | cut -d\" -f1)&
	   fi
	   if [ $mpv = 0 ] && [ $yad != 0 ] ; then
	  temp=0
	  chosenbyheaven=$(echo yad --list --title=\"$names\" --column=\"Выбор торрента\" $(until [ $temp = $all ];do
        let ' temp = temp + 1 '
        echo \"Серии $(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*series//g | cut -d\} -f1 | rev | cut -d\" -f2 | rev), $(echo $resp sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*quality//g | cut -d\} -f1 | sed s/.*string//g | cut -d\" -f3), $(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*seeders//g | cut -c3- | cut -d\, -f1) сидов, $(bc<<<"scale=2;$(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*total_size\"://g | cut -d, -f1)/1073741824")GiB, $(date --date @$(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*uploaded_timestamp\"://g | cut -d, -f1))\"
        done) --hide-header --height=$((151 + $temp * 24)) --text-align=center --center --width=$(($(echo $(xdpyinfo | grep 'dimensions:' | cut -dx -f1 | rev | cut -c1-7) | rev) * 2 / 5)) --buttons-layout=center --text-align=center | bash)
        until [ "$temp" -ge $all ]; do
        let ' temp = temp + 1 '
        #echo $temp
        kill $descpid 2>> /dev/null&
        if [ "$(echo $(echo $chosenbyheaven | cut -d, -f2))"  = "$(echo $resp sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*quality//g | cut -d\} -f1 | sed s/.*string//g | cut -d\" -f3)" ]; then
        $(grep application/x-bittorrent /usr/share/applications/mimeinfo.cache | cut -d\= -f2 | cut -d. -f2) http://cvd.anilib.top$(echo $resp | sed s/.*\"torrents\"\://g | sed s/},{/^/g | cut -d\^ -f$temp | sed s/.*\"url\"\:\"//g | cut -d\" -f1)&
        break;fi
        done
	   fi
	   if [ $mpv = 1 ]; then
	   #realnum=000$temp;num=$(echo $num | cut -c${#temp}-7)
	   playlists=$(( $temp - 1 ))												# REDACTEDы REDACTEDть
	   if (( debug > 1 )); then echo seems mpv=1, num=$num, temp=$temp; fi		# REDACTED REDACTEDть
	   temp=1																	# какого REDACTED оно не пашет то REDACTEDть?
	   echo '#EXTM3U' > videos.m3u
	   until [ $temp = $(( $last + 1 )) ]										# REDACTEDы REDACTEDть
	   do																		# REDACTED REDACTEDть
