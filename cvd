#!/bin/bash
#cvd alpha 0.5 by Gribanov "Здесь ты ищешь тот шар золотой..."
start=$(date +%s) # требуеться для посчёта времени затраченного на выполнение
if [ "$DISPLAY" = '' ] ;then yad=0;if [ -d /data/data/com.termux/ ];then echo При возникновении проблем с Mx Player или LazyMedia
echo используйте другй плеер, например Kmplayer или VLC;termux=1;if [ "$yad" = '1' ]; then echo Графика в термуксе? ого!; yad=termux;fi;fi;fi
rm -rf .cvd*tmp
touch .cvd.link.tmp
argsV='-y -c copy -bsf:a aac_adtstoasc' # аргументы для скачивания видео (только ffmpeg)
argsA='-y -bsf:a aac_adtstoasc' # аргументы для скачивания аудио, впервые появились в альфа pre-0.5, пока им применение не найдено
link=$@ # костыльный ком, нужно будет его убрать.. наверное...
cont=mkv # контейнер для скачанных файлов
downtime=10 # задежка между попытками скачать новую серию
debug=0 # debug level (вот это поворот)
gospod='Чего желаете, господин?' # входное сообщение
cvdw=213 # Ширина маленьких окон
progname='CVD 0.5' # кодовое название
#$(cat ~/.config/cvd)
if [ "$yad" = 0 ] && [ "$yad" != termux ]; then :; else
yad=1 # GUI
fi
if [ $yad != 0 ] && [[ "$@" = '' ]] ; then
										curl https://darklibria.it/ -Lo .cvd.dark.tmp 2> .cvd.curl.tmp
										gcut=$(echo $(cat .cvd.dark.tmp) | sed 's/.*genre//g')
										ycut=$(echo $(cat .cvd.dark.tmp) | sed 's/.*year//g')
										#genres=$(echo $gcut | sed 's/<\/select>.*//g' | sed s/'<\/option> <option>'/':chk" --field=$"'/g | sed s'/<\/option>'/':chk"'/g | sed s/'" size="6" data-live-search="true"> <option>'/'--field=$"'/g);while [[ "$genres" = *DOCTYPE* ]]; do genres=$(echo $(curl https://darklibria.it/ -o .cvd.dark.tmp 2> .cvd.curl.tmp;cat .cvd.dark.tmp;gcut=$(echo $(cat .cvd.dark.tmp) | sed 's/.*genre//g')) | sed 's/<\/select>.*//g' | sed s/'<\/option> <option>'/':chk" --field=$"'/g | sed s'/<\/option>'/':chk"'/g | sed s/'" size="6" data-live-search="true"> <option>'/'--field=$"'/g); done
										genres=$(echo $gcut | sed 's/<\/select>.*//g' | sed s/'<\/option> <option>'/':chk" --field=$"'/g | sed s'/<\/option>'/':chk"'/g | sed s/'" size="6" data-live-search="true"> <option>'/'--field=$"'/g | sed s/' data-live-search=\"true\"> <option>'/--field=/g );if [[ "$genres" = *DOCTYPE* ]]; then "$0";exit;fi
										years=$(echo $ycut | sed 's/<\/select>.*//g' | sed s/'<\/option> <option>'/':chk" false --field=$"'/g | sed s'/<\/option>'/':chk" false '/g | sed s/'" size="6" data-live-search="true"> <option>'/'--field=$"'/g | sed s/' data-live-search=\"true\"> <option>'/--field=/g)
										echo yad --plug=$start --tabnum=2 --form --columns=3 $genres --text=\"Если ни один из жанров не выбран - поиск будет произведён по каждому\". \> .cvd.tf.tmp | bash &
										echo yad --plug=$start --tabnum=3 --form --columns=3 $years --field="Сезон":CB 'Любой!Зима!Весна!Лето!Осень' --text=\"Если ни один год не выбран - поиск будет произведён по каждому\". \> .cvd.years.tmp > yad-alert2;bash yad-alert2 &
fi
#cat /usr/local/bin/cvd | sed 's/Выберите/Оберіть/g;s/Описание/Опис/g;s/желаете/бажаєте/g;s/Чего/Чого/g;s/господин/господине/g;s/Отправить/Відправити/g;s/желаю/бажаю/g;s/оиск/ошук/g;s/Мне/Мені/g;s/повезёт/пощастить/g;s/Искомое/Шукане/g;s/временам/рокам та сезонам/g;s/Если/Якщо/g;s/ни один/жодного/g;s/из/з/g;s/ров/рів/g;s/ран/рано/g;s/будет/буде/g' | bash
#mpvargs='--hwdec=auto'
#var=$(echo "$var" | sed 's/ /+/g' );echo https://mangalib.me/$(curl 'https://mangalib.me/search?type=manga&q='$var | sed s/.*'slug":"'//g | cut -d\" -f1)
