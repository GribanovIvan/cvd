	   #videos="$videos https://"$ll"videos/ts/$id/$num$res/playlist.m3u8"
	   echo \#EXTINF:-1,$names $temp $(echo $res | cut -c2- | tr a-z A-Z) >> videos.m3u
	   echo https://"$ll"videos/ts/$id/$num$res/playlist.m3u8 >> videos.m3u
	   let 'temp = temp + 1'
	   #echo $videos
	   done
	   echo \#EXT-X-ENDLIST >> videos.m3u
	   #echo debug exit. you are using broken program.; exit
	   #if [ -z $(cat .cvd.reso.tmp) ]; then :;else resget=', '
	   #fi
	   #if [ $first != $last ]; then ser=', серия '
	   #else
	   #temp=''
	   #fi #videos $mpvargs --force-media-title="$(echo $names$ser$temp$resget$(cat .cvd.reso.tmp))"
	   #names=$(echo $names)
	   #if [ "$playlists" -ge 1 ]; then
	   #echo $names
	   #echo $resget
	   #cat .cvd.reso.tmp
	   if [ "$termux" = 1 ]; then termux-open videos.m3u
	   else
	   mpv videos.m3u $mpvargs -cache=auto --playlist-start=$playlists #--force-media-title="$(echo $names$resget$(cat .cvd.reso.tmp))"
	   fi
	   #else
	   #mpv $videos $mpvargs -cache=auto --force-media-title="$(echo $names$resget$(cat .cvd.reso.tmp))"
	   #fi
	fi
	elif [ $listing = jutsu ] && [ $mpv = 1 ]; then
	echo $jutres > .cvd.jutres.tmp
	mpv --user-agent='' $video $mpvargs --force-media-title="$(echo $names, серия $num, $(cat .cvd.reso.tmp))"
	elif [ $listing != anilbria ] && [ $mpv = 0 ] && [ "$temp" != 0 ]; then #echo щас сREDACTEDдем..
	until [ "$err" = 0 ]
	do
	temp=$(yad --file --save --confirm-overwrite='Вы уверены, что хотите перезаписать выбранный файл?' --filename="$title")
	if [ -z "$temp" ]; then err=1
	else err=0;fi
	done
	#if [[ $temp != *.mkv ]] || [[ $temp != *.mp4 ]] || [[ $temp != *.avi ]] || [[ $temp != *.$cont ]]; then temp=$temp.$cont;fi
	cont=$(echo $video | cut -d? -f1 | rev | cut -d. -f1 | rev)
	if [[ "$temp" != *.$cont ]] && [ "$listing" != anilibria ]; then temp="$temp".$cont;fi
	curl $video -L -A '' -o "$temp"
	fi
if (( $debug < 1 )); then rm -rf .cvd.*tmp; rm -rf yad-alert*
elif (( $debug > 0 )); then echo first=$first
echo last=$last
echo nums=$nums
echo allnum=$allnum
echo lfhd=$lfhd # он не помнит слова "да" и слова "нет"
