#curl https://anime.anidub.life/player/index.php?vid=/s1/10564/1/1.mp4 -e https://anime.anidub.life -A '' 2> .cvd.curl.tmp| grep -i video.php | grep -i videoUrl | cut -d? -f2 | cut -d\' -f1
#curl https://amai.tv/embeds/search 2> .cvd.curl.tmp | grep 8 | wc -l #curl https://amai.tv/embeds/search 2> .cvd.curl.tmp | sed ':a;N;$!ba;s/,\n/,/g' | grep 8 | wc -l
#curl $(echo https://amai.tv$(curl https://amai.tv/anime/doktor-stoun 2>> .cvd.curl.tmp | grep -i embeds | cut -d\" -f2)) | grep -i Сезон | wc -l
#[ivan@HP255-G7 tmp]$ ses=2
#[ivan@HP255-G7 tmp]$ ser=5
#[ivan@HP255-G7 tmp]$ req=стоун
#[ivan@HP255-G7 tmp]$ mpv http:$(curl $(echo https://amai.tv$(curl https://amai.tv$(curl https://amai.tv/embeds/search 2>> .cvd.curl.tmp | sed ':a;N;$!ba;s/,\n/,/g' | grep -i $"$req" | cut -d: -f5 | cut -d\" -f2) 2>> .cvd.curl.tmp | grep -i embeds | cut -d\" -f2)) 2>> .cvd.curl.tmp| sed ':a;N;$!ba;s/\n/@/g' | sed 's/.*$ses\ Сезон//g' | sed 's/.*\[//g' | sed 's/\]}.*//g' | sed s/'@'/\\n/g | grep -i "Серия $ser" | cut -d\" -f8)
#mpv http:$(curl $(echo https://amai.tv$(curl https://amai.tv$(curl https://amai.tv/embeds/search 2>> .cvd.curl.tmp | sed ':a;N;$!ba;s/,\n/,/g' | grep -i стоун | cut -d: -f5 | cut -d\" -f2) 2>> .cvd.curl.tmp | grep -i embeds | cut -d\" -f2)) 2>> .cvd.curl.tmp| sed ':a;N;$!ba;s/\n/@/g' | sed 's/.*2\ Сезон//g' | sed 's/.*\[//g' | sed 's/\]}.*//g' | sed s/'@'/\\n/g | grep -i 'Серия 5' | cut -d\" -f8)
#curl http://rezkery.com/animation/drama/2483-evangelion-2.22-ty-ne-proydesh.html -A cvd --compressed 2> .cvd.curl.tmp| sed s/.*Ultra]//g | sed s/:hls.*//g | sed s/'\\\/'/'\/'/g
#qbittorrent http://shiza-project.com$(curl https://shiza-project.com/releases/rwby-tv-6 | grep \"torrent-counter\" | sed s/.*href=\"//g | cut -d\" -f1)
#curl http://rezkery.com/engine/ajax/search.php --compressed --data-raw 'q=евангелион'
#mpv $(curl https://play.animegost.org/$(curl https://animevost.org/tip/tv/2708-kimetsu-no-yaiba-2nd-season.html | grep 1\ серия\" | cut -d\" -f4) | grep mp4 | tac | cut -d'
#' -f1 | sed s/.*href//g | cut -d\" -f2)
mpv --version > /tmp/.cvd.mpv.tmp&
if [ -z "$2" ]; then name=video # если скачиваться видео со страшного ресурса, то его
				else name=$2 #    имя будет вторым аргументом, если не указан - video
fi
if (( $debug > 2 )); then echo first done.;fi
if [ -z "$link" ] ; then
					until [ "$err" = 0 ]; do																																	# я прибавил 3 пиксиля. И что?
					if [ $yad != 0 ]; then yad --form --field=$"$gospod" --field=Поиск\ на\ jutsu:chk --field=Поиск\ на\ AniLibria:chk --width=$cvdw --plug=$start --tabnum=1 '' true true > .cvd.link.tmp &
					#yad --notebook --key=$start --tab=$"Искомое" --tab=$"По жанрам" --tab=$"По временам" --title="$progname" --buttons-layout=center --button='Отправить!answer:0' --button='Не желаю.!call-stop:1' --button='Мне повезёт!star-shape:yad --text=Поздравляем\!\!\ вы\ нажали\ на\ волшебную\ кнопку\ удачи\!\!\ теперь\ вам\ будет\ сопутствовать\ удача. --center --no-buttons' --text-align=center --center 
					yad --notebook --key=$start --tab=$"Искомое" --tab=$"По жанрам" --tab=$"По временам" --title="$progname" --buttons-layout=center --button='Отправить!gtk-apply:0' --button='Не желаю.!call-stop:1' --button='Мне повезёт!starred:2' --button='Последние!clock:3' --text-align=center --center
					case $? in
   1) exit;;
#   2) link=$(echo $(curl -l https://darklibria.it/random) 2>> .cvd.curl.tmp | sed 's/.*href="\/release\///g' | cut -d\" -f1)
	2) link=$(curl http://api.anilibria.tv/v2.9.1/getRandomTitle?filter=id 2> .cvd.curl.tmp | cut -d\: -f2 | cut -d\} -f1) #
#	2) link=$(curl -Lf 'https://al.sdev.xyz/public/api/index.php' -F query=random_release 2>> .cvd.curl.tmp | cut -d\" -f8
   #link=pet-na-kanojo REDACTEDня, которая есть и на либре, и на джутсу, кстати годное аниму
   findby=id;err=0;anilibria=1
   #echo gachi fix me too and give debug sex
   ;;
   252) exit;;
   3) link=showlast;;
   0) link=$(cat .cvd.link.tmp | cut -d\| -f1);;
esac
					tf=$(cat .cvd.tf.tmp 2>> .cvd.tmp)							# эта конструкция спршаривает.
					years=$(cat .cvd.years.tmp 2>> .cvd.tmp)
					all=$(echo "$tf" | grep -o "|" | wc -l)
					tfr=$(echo $gcut | sed 's/<\/select>.*//g' | sed s/'<\/option> <option>'/\|/g | sed s'/<\/option>'/'|'/g | sed s/'" size="6" data-live-search="true"> <option>'/''/g)
					yearr=$(echo $ycut | sed 's/<\/select>.*//g' | sed s/'<\/option> <option>'/\|/g | sed s'/<\/option>'/'|'/g | sed s/'" size="6" data-live-search="true"> <option>'/''/g)
					if [ -z $tf ]; then :;else
until [ "$temp" = "$all" ]
do
let 'temp = temp + 1' # В ПЕРЕМЕННОЙ extra ВСЯ REDACTEDНЯ, ГЛАВНОЕ НЕ ЗАБЫТЬ
if [ $(echo $tf | cut -d\| -f$temp) = TRUE ]; then extra=$extra\&genres\=$(echo $tfr | cut -d\| -f$temp);fi
if [ "$(echo $years | cut -d\| -f$temp)" = TRUE ]; then extra=$extra\&year\=$(echo $yearr | cut -d\| -f$temp);fi
done
fi
																#приветствие.
						else
						echo $gospod #				консольное	#приветствие.
						read link # считыватель введенных аргуме.. А REDACTED его, просто костыль
					fi
					link=$link | sed 's/^[ \t]*//;s/[ \t]*$//' | cut -d? -f1 #  отчикрыживатель пробелов в начале и конце. Спасибо, Hfib а потом откусыватель аргументов
					while [[ "$link" = *\ \ * ]]; do var=$(echo "$link" | sed s/'  '/' '/g);done
					if [ -z "$link" ]; then
					err=1;gospod='Самый умный? введи чёт.' #для глупых.
					if [ $yad != 0 ];then yad --width=$cvdw  --center --text=$"без буков не как." --text-align=center --title="$progname" &
					exit;fi
					else err=0
					fi
									done
fi
if [ "$link" = random ]; then link=$(curl http://api.anilibria.tv/v2.9.1/getRandomTitle?filter=id 2>> .cvd.curl.tmp| cut -d\: -f2 | cut -d\} -f1);fi
if (( $debug > 2 )); then echo first2 done.;fi
if [ "$link" = q ] && [[ $@ != q ]] ; then echo Выход...;exit # выход при вводе q
    elif [[ "$link" = http*://*.libria.fun/videos/ts/* ]] ; then							#доREDACTEDа
	 num=$(( ${#link} - 20 ))											#умный считатор
     num=$(echo $link | cut -c$num-$(let 'num = num + 6'; echo $num))	#номера
	 if [[ "$num" = */* ]] ; then num=$(echo $num | cut -c4-7)-HD		#для
     elif [[ "$num" = *-sd ]] ; then num=$(echo $num | cut -c1-4)-360p	#анилибры
     elif [[ "$num" = *-hd ]] ; then num=$(echo $num | cut -c1-4)-1080p	#который я оффицально переделал уже в третий раз UPD: четвёртого не будет, наREDACTED надо?
	elif [ "$err" != 0 ]; then
	echo Неизвестная ошибка связанная с получением качества аниме!			#когда ниREDACTED понятного не случилось
	echo Прошу, напишите мне, ХАЛАДОС#8335
    fi
	echo Скачивание началось! ожидайте..
	trap "int=1; echo Прерванно." INT
	ffmpeg -i $link $argsV $num.$cont #2> .cvd.tmp       устарело
	if [ "$int" = 1 ]; then rm -rf $num.$cont;exit		#в
	else exit;fi # скачивалка, аргументы идут в .cvd.tmp	0.5
	elif [[ "$link" = *.m3u8 ]] ; then # хандлер m3u8 потоков
	ffmpeg -i $link $argsV $name.$cont # скачивалка
	elif [[ $link = *dark*libria.it/release/* ]]; then #ОБНАРУЖЫЖЕВАТИТИВАТЕЛЬ анилибрЫ REDACTEDа, переделано под новую дарк либрию..
	findby=code;err=0;listing=anilibria
	link=$(echo $link | cut -d: -f2)
	if [[ $link = //* ]]; then link=$(echo $link | cut -d \/ -f5)
	else link=$(echo $link | cut -d \/ -f3)
	fi
	elif [[ "$link" = http*://video.sibnet.ru/shell.php?videoid=* ]]; then
	listing=sibnet
	curl "$link" 2>> .cvd.curl.tmp > .cvd.tmp
	video=$(printf https://$(curl https://video.sibnet.ru$(cat .cvd.tmp | grep -i .mp4 | sed s/.*player.src//g | cut -d\" -f2) -e "$link" -I 2>> .cvd.curl.tmp | grep -i Location | cut -c13- | rev | cut -c2- | rev))
	title=$(cat .cvd.tmp | grep -i data-title | cut -d\\ -f2 | cut -c2- | sed s/\amp\;//g)
	listing=raw
	echo '|FALSE|FALSE|' > .cvd.link.tmp
	elif [[ "$link" = http*://video.sibnet.ru/video* ]]; then
	link=https://video.sibnet.ru/shell.php?videoid=$(echo $link | sed s/.*video//g | cut -d- -f1) # https://video.sibnet.ru/video3464851-_AniDub___Gyakuten_Saiban__Sono_Shinjitsu__Igi_Ari____Pervoklassnyiy_advokat__TV_2___01___Lonely_Dragon_/
	listing=sibnet
	curl "$link" 2>> .cvd.curl.tmp > .cvd.tmp
	video=$(printf https://$(curl https://video.sibnet.ru$(cat .cvd.tmp | grep -i .mp4 | sed s/.*player.src//g | cut -d\" -f2) -e "$link" -I 2>> .cvd.curl.tmp | grep -i Location | cut -c13- | rev | cut -c2- | rev))
	title=$(cat .cvd.tmp | grep -i data-title | cut -d\\ -f2 | cut -c2- | sed s/\amp\;//g)
	listing=raw
	echo '|FALSE|FALSE|' > .cvd.link.tmp
	#elif [[ "$link" = http*://anime.anidub.life/anime/full/*.html ]]; then
	#listing=anidub
	elif [[ "$link" = *rezka.ag/*/*/*-*.html* ]]; then
	link=$(echo $link | sed s/rezka.ag/rezkery.com/g | sed s/https/http/g)
	mpv $(curl "$link" -A cvd --compressed 2> .cvd.curl.tmp| sed s/.*Ultra]//g | sed s/:hls.*//g | sed s/'\\\/'/'\/'/g | rev | cut -d\; -f1 | rev)
	exit
	elif [[ $link = *anilibria.tv/release/* ]]; then #тоже ОБНАРУЖЫЖЕВАТИТИВАТЕЛЬ анилибрЫ
	findby=code;err=0;listing=anilibria
	link=$(echo $link | cut -d: -f2)
	if [[ $link = //* ]]; then link=$(echo $link | cut -c28- | cut -d. -f1)
	else link=$(echo $link | cut -c24-)
	fi
	 elif (echo "$link" | grep -E -q "^?[0-9]+$") && [ ${#link} = 3 ] || [ ${#link} = 4 ] && (echo "$link" | grep -E -q "^?[0-9]+$");
	  then													# проверялка, введён айдишник или нет. в своё время эта штука заставила сделать меня реврайт.
	 if (( $debug > 2 )); then echo reached target 2;fi
	  findby=id;err=0;listing=anilibria # прадед, сыщик анилибры.
	  if (( debug > 1 )); then echo $link has been used.;fi
	  fi
	  if (( debug > 2 )); then echo $link has been used.;fi
	  if (( $debug > 2 )); then echo where im;fi
	if [ -z "$listing" ] && [ "$(cat .cvd.link.tmp | cut -d\| -f3)" = TRUE ] || [ "$(cat .cvd.link.tmp | cut -d\| -f3)" = '' ]; then
	codeplus=$(echo $link | sed s/' '/+/g | sed s/-/+/g)			# рандомная REDACTEDня!! в бой!!, а если серьёзно ищет альтернативы.
	#curl https://darklibria.it/search?find=$codeplus$extra 2> .cvd.curl.tmp | grep -i '<a href="/release/' |  sed -e n\;d > .cvd.tmp
	#curl -Lf https://al.sdev.xyz/public/api/index.php 2> .cvd.curl.tmp -F query=search -F search="$link" | jq 'if .status then .data else error(.error) end' > .cvd.tmp #| grep -o '"code":' | wc -l
		if [ $yad = 0 ];then printf "Отправка запроса.."
	#else echo "# Отправка запроса"
	fi
	if [ "$link" = showlast ]; then
	if [ $yad != 0 ]; then
	curl http://api.anilibria.tv/v2/getChanges?filter=names,id\&show_hidden=1\&limit=$(( ($(xdpyinfo | grep 'dimensions:' | cut -dx -f2 | cut -d' ' -f1) - 170) /26 )) 2> .cvd.curl.tmp | jq > .cvd.tmp
	else
	curl http://api.anilibria.tv/v2/getChanges?filter=names,id\&show_hidden=1\&limit=$(( $(stty size | cut -d' ' -f1) - 2)) 2> .cvd.curl.tmp | jq > .cvd.tmp
	fi
	else
	curl http://api.anilibria.tv/v2/searchTitles?search=$(echo $link | jq -sRr @uri | rev | cut -c4- | rev)\&filter=names,id\&limit=-1\&show_hidden=1 2> .cvd.curl.tmp | jq > .cvd.tmp
	fi
	sed s/'\\\"'/двойная\ хрень/g .cvd.tmp > .cvd.many.tmp
	cat .cvd.many.tmp | grep \"ru\" | cut -d\" -f4 > names-ru;cat .cvd.many.tmp | grep \"en\" | cut -d\" -f4 > names-en;cat .cvd.many.tmp | grep \"id\"\: | cut -d: -f2 > id
	#temp=$(cat .cvd.tmp| cut -d'"' -f2 | cut -d\/ -f3)
	#all=$(cat .cvd.tmp | grep -c '"code":')
	all=$(wc -l id | cut -d' ' -f1)
	if [ $(( all > 1 )) = 1 ]; then
	h=$(($all * 25 + 92))
	#cat .cvd.tmp | cut -d'"' -f6 > .cvd.many.tmp
	temp=0
	rm -rf .cvd.many.tmp
    if [ $yad != 0 ]; then
	w=$(($(($(grep -Em1 "^.{$(wc -L < names-ru)}\$" names-ru | wc -c) +$(grep -Em1 "^.{$(wc -L < names-en)}\$" names-en | wc -c)))* 47 / 9)) # прочитай это
	if [ $(( w < cvdw )) = 1 ]; then w=$cvdw;fi
echo "yad --title=\$\"$progname\" --text-align=center --list --center --height=$h --width=$w --column=Выберете\ релиз \\" > yad-alert3
until [ $temp = $all ];do let 'temp = temp + 1'; echo $(printf \";cat names-ru | cut -d'
' -f $temp; echo '/';cat names-en | cut -d'
' -f $temp;printf \"\ \\) | sed s/\ \'/\'/g >> yad-alert3;done
#echo \#all=$all >> yad-alert3
sed s/двойная\ хрень/'\\\"'/g yad-alert3 > yad-alert2
mv yad-alert2 yad-alert3
#chosenbyheaven=$(bash yad-alert3 | cut -d\" -f2 | sed 's/\ \/\ .*//g')
#curl https://darklibria.it/search?find=$codeplus$extra 2> .cvd.curl.tmp | grep -i '<a href="/release/' |  sed -e n\;d > .cvd.tmp
#	echo $chosenbyheaven
#exit
link=blank	#													Не верю я, нет, не верю, в то что использовал апи вэ2, в то что в луже багов утонул сей апи а ведь правдой было это. Не верю я, нет, не верю я что использовал таки апи вэ2, багкостыль не для нас, пусть и вэ1 не угас, знаю я ман перепас.
link=$(sed $(grep -n "$(bash yad-alert3 > .cvd.tmp; echo $? > .cvd.1.tmp; cat .cvd.tmp| cut -d\" -f2 | sed 's/\ \/\ .*//g')" names-ru | head -n 1 | cut -d: -f1)\!D id | cut -c2-)
	 if [ $(cat .cvd.1.tmp) = 1 ] || [ $(cat .cvd.1.tmp) = 252 ] ;then exit;fi
	 echo $temp
#curl -Lf 'https://al.sdev.xyz/public/api/index.php' -F query=release -F id=$link 2>> .cvd.curl.tmp | jq 'if .status then .data else error(.error) end' > .cvd.tmp
else
#echo $all
printf "\rНайдено несколько релизов.. Выберите один\n"
temp=0
until [ "$temp" = $all ]; do let ' temp = temp + 1';echo $temp\) $(cat names-ru | cut -d'
' -f"$temp") / $(cat names-en | cut -d'
' -f"$temp"); done
read all #;temp=$all
link=$(sed $all\!D id | cut -c2-)
fi
#curl -Lf 'https://al.sdev.xyz/public/api/index.php' -F query=release -F id=$link 2>> .cvd.curl.tmp | jq 'if .status then .data else error(.error) end' > .cvd.tmp
