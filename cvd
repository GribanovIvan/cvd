	  if [ $temp = $allnum ]; then break
	  fi
	  let 'temp = temp + 1'
	  done
	 if [ "$int" = int ]; then rm -rf $num.$cont; fi
	 fi
	 if [ "$temp" = "$down" ] && [ $mpv = 0 ];
	 then
	 num=000$temp;num=$(echo $num | cut -c${#temp}-7)
	 temp=0
