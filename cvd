	if [ "$temp" = '480p|' ]; then temp=2;fi
	if [ "$temp" = '720p|' ]; then temp=3;fi
	if [ "$temp" = '1080p|' ] ; then temp=4;fi
	if [ "$temp" = '360p|' ]; then temp=1;fi
